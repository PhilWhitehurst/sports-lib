{
  "name": "quantified-self-lib",
  "version": "3.4.25",
  "description": "A Library to for processing GPX, TCX, FIT and JSON files from services such as Strava, Movescount, Garmin, Polar etc",
  "keywords": [
    "gpx",
    "tcx",
    "fit",
    "activities",
    "json",
    "suunto",
    "garmin",
    "polar",
    "export",
    "import",
    "reader",
    "library",
    "utility",
    "parser"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc",
    "test": "jest",
    "test-coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jimmykane/quantified-self-lib.git"
  },
  "bugs": {
    "url": "https://github.com/jimmykane/quantified-self-lib/issues"
  },
  "homepage": "https://github.com/jimmykane/quantified-self-lib",
  "author": "Dimitrios Kanellopoulos",
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "@types/node": "^13.1.2",
    "fast-xml-parser": "^3.15.1",
    "fit-file-parser": "^1.6.16",
    "geolib": "^3.2.1",
    "lowpassf": "^0.5.0",
    "moving-median": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@types/xmldom": "^0.1.29",
    "jest": "^24.9.0",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4",
    "xmldom": "^0.2.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    }
  }
}
